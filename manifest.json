{
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "toggle-unlock": {
         "suggested_key": {
            "default": "Alt+Shift+U"
         },
         "description": "Toggle Unlock Copy on the current site"
      },
      "toggle-force": {
         "suggested_key": {
            "default": "Alt+Shift+F"
         },
         "description": "Toggle Force Mode on the current site"
      },
      "toggle-session": {
         "suggested_key": {
            "default": "Alt+Shift+S"
         },
         "description": "Toggle Session Only for the current tab"
      }
   },
   "action": {
      "default_icon": {
         "16": "images/16px.png",
         "32": "images/32px.png",
         "48": "images/48px.png",
         "128": "images/128px.png"
      },
      "default_popup": "main.html",
      "default_title": "RightClick Unbound"
   },
   "description": "Restore right-click and copy on restrictive pages with quick toggles and an optional force mode.",
   "icons": {
      "128": "images/128px.png",
      "16": "images/16px.png",
      "32": "images/32px.png",
      "48": "images/48px.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "RightClick Unbound",
   "options_ui": {
      "page": "pages/options.html",
      "open_in_tab": true
   },
   "permissions": [ "tabs", "activeTab", "storage", "scripting", "contextMenus" ],
   "host_permissions": [ "\u003Call_urls>" ],
   "short_name": "RC Unbound",
   "author": "Unbound Tools",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.0"
}
